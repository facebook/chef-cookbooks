<% node['fb_dnsmasq']['config'].to_hash.each do |key, val|
     if val
       if val.is_a?(Array)
         vals = val
       else
         vals = [val]
       end
       vals.each do |v| -%>
<%=      key %><% if v %>=<%= v %><% end %>
<%     end -%>
<%   else -%>
<%=    key %>
<%   end -%>
<% end -%>
