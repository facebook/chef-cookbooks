<% node['fb_vsftpd']['config'].to_hash.each do |key, val| -%>
<%   if val.kind_of?(TrueClass) -%>
<%=    key %>=YES
<%   elsif val.kind_of?(FalseClass) -%>
<%=    key %>=NO
<%   else -%>
<%=    key %>=<%= val.to_s %>
<%   end -%>
<% end -%>
