NETWORKING=yes
HOSTNAME=<%= node['fqdn'] %>
LINKDELAY=<%= node['fb_network_scripts']['linkdelay'] %>
NETWORKING_IPV6=yes
IFDOWN_ON_SHUTDOWN=no
<% if node['fb_network_scripts']['v6_default_gw'] %>
IPV6_DEFAULTGW="<%= node['fb_network_scripts']['v6_default_gw'] %>"
<% end %>
