[<%= @section %>]
<% keys = node['fb_systemd'][@config].to_hash.keys
   keys.sort.each do |key|
     value = node['fb_systemd'][@config][key]
     if value.is_a?(TrueClass)
       v = 'yes'
     elsif value.is_a?(FalseClass)
       v = 'no'
     else
       v = value
     end %>
<%=  key %>=<%= v %>
<% end %>
